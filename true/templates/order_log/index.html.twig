{% extends 'base.html.twig' %}

{% block title %}OrderLog index{% endblock %}

{% block body %}
    <h1>OrderLog index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Quantity</th>
                <th>BuyingPrice</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for order_log in order_logs %}
            <tr>
                <td>{{ order_log.id }}</td>
                <td>{{ order_log.quantity }}</td>
                <td>{{ order_log.buyingPrice }}</td>
                <td>
                    <a href="{{ path('order_log_show', {'id': order_log.id}) }}">show</a>
                    <a href="{{ path('order_log_edit', {'id': order_log.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('order_log_new') }}">Create new</a>
{% endblock %}
